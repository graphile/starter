    "@app/graphql": "0.0.0",
    "@app/lib": "0.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.28",
    "antd": "5.2.3",
    "dayjs": "^1.11.7",
    "graphql": "^15.8.0",
    "lodash": "^4.17.21",
    "net": "^1.0.2",
    "next": "^15.2.4",
    "nprogress": "^0.2.0",
